{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/Images/(.*)",
      "dest": "/public/Images/$1"
    }
  ],
  "static": {
    "directory": "public"
  }
}
